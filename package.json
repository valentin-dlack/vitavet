{
  "name": "vitavet",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "pnpm --filter api test && pnpm --filter web test:run",
    "dev": "concurrently -k -n API,WEB -c green,blue \"pnpm --filter ./apps/api start:dev\" \"pnpm --filter ./apps/web dev\"",
    "build": "pnpm --filter api build && pnpm --filter web build",
    "lint": "pnpm --filter api lint && pnpm --filter web lint",
    "format": "pnpm --filter api format && pnpm --filter web format",
    "test:e2e": "pnpm --filter api test:e2e",
    "coverage": "pnpm --filter api test:cov && pnpm --filter api test:e2e:cov && pnpm --filter web test:cov",
    "coverage:summary": "node scripts/coverage-summary.mjs",
    "prepare": "husky",
    "commit-msg": "husky",
    "db:dump": "node scripts/db-dump.mjs",
    "db:reset": "pnpm --filter api db:reset",
    "api:seed": "pnpm --filter api seed:dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "concurrently": "^9.2.0",
    "husky": "^9.1.7"
  },
  "dependencies": {
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "jwt-decode": "^4.0.0",
    "path": "^0.12.7"
  }
}
